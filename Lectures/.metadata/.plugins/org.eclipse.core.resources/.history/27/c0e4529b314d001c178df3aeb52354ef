package Filter;
import java.util.List;
import java.util.ArrayList;


public class CriteriaMale {

	public List<Person> meetCriteria (List<Person> people){
		List <Person> males = new ArrayList<>();
		for ( Person p : people) {
			if (p.getGender().equalsIgnoreCase("male")) {
				males.add(p);
			}
		}
		return males;

}
